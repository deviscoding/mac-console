{
    "name": "deviscoding/mac-console",
    "description": "Parent class with convenience functions for creating macOS CLI commands using the Symfony console component.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "AMJones",
            "email": "am@jonesiscoding.com"
        }
    ],
    "version": "1.8",
    "autoload": {
        "psr-4": {
            "DevCoding\\Mac\\": "src"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "deviscoding/objection",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/deviscoding/objection",
                    "type": "git",
                    "reference": "main"
                }
            }
        }
    ],
    "require": {
        "php": "^7.0",
        "ext-json": "*",
        "ext-posix": "*",
        "jonesiscoding/base-console": "^4.4",
        "symfony/process": "^4.4",
        "symfony/yaml": "^4.4",
        "deviscoding/objection": "dev-master"
    }
}
